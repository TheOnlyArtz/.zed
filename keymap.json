[
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-s": "workspace::SaveWithoutFormat",
      "alt-t": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-right": "pane::ActivateNextItem",
      "alt-left": "pane::ActivatePrevItem",
      "ctrl-1": "workspace::ActivatePreviousPane",
      "ctrl-3": "workspace::ActivateNextPane",
      "ctrl-e ctrl-g": "project_symbols::Toggle",
      "ctrl-enter": "editor::NewlineAbove",
      "ctrl-e": "editor::GoToDefinition",
      "ctrl-r": "pane::SplitLeft",
      "ctrl-i": "editor::ToggleInlayHints"
    }
  }
]
